<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Camunda.OpenApi.Client</AssemblyName>
    <PackageId>Camunda.OpenApi.Client</PackageId>
    <OutputType>Library</OutputType>
    <Authors>Silvan Brenner</Authors>
    <Company>OpenAPI</Company>
    <AssemblyTitle>OpenAPI Library</AssemblyTitle>
    <Description>Camunda C# Client generated from OpenApi</Description>
    <Copyright></Copyright>
    <RootNamespace>Camunda.OpenApi.Client</RootNamespace>
    <Version>1.0.0</Version>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\Camunda.OpenApi.Client.xml</DocumentationFile>
    <RepositoryUrl>https://github.com/silvanbrenner/camunda-csharp-client.git</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageReleaseNotes></PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/silvanbrenner/camunda-csharp-client.git</PackageProjectUrl>
    <PackageTags>camunda bpmn rest openapi</PackageTags>
    <PackageVersion>1.0.1</PackageVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="JsonSubTypes" Version="1.7.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
    <PackageReference Include="RestSharp" Version="106.11.4" />
    <PackageReference Include="Polly" Version="7.2.0" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.7.0" />
  </ItemGroup>

</Project>
